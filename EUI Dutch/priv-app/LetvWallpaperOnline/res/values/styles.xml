<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CardView">
        <item name="cardBackgroundColor">@color/cardview_light_background</item>
        <item name="cardCornerRadius">@dimen/cardview_default_radius</item>
        <item name="cardElevation">@dimen/cardview_default_elevation</item>
        <item name="cardMaxElevation">@dimen/cardview_default_elevation</item>
        <item name="cardUseCompatPadding">false</item>
        <item name="cardPreventCornerOverlap">true</item>
    </style>
    <style name="CardView.Light" parent="@style/CardView">
        <item name="cardBackgroundColor">@color/cardview_light_background</item>
    </style>
    <style name="CardView.Dark" parent="@style/CardView">
        <item name="cardBackgroundColor">@color/cardview_dark_background</item>
    </style>
    <style name="LeTextStyle">
        <item name="android:textSize">@dimen/item_recomend_tv_music_index_front_size</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@color/item_recommend_card_background_color</item>
        <item name="android:fontFamily">helve-neue-ultralight</item>
        <item name="android:layout_marginEnd">@dimen/item_recomend_margin_size</item>
    </style>
    <style name="LeTextStyleThin">
        <item name="android:textSize">16.0sp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@color/item_recommend_card_background_color</item>
        <item name="android:fontFamily">helve-neue-thin</item>
        <item name="android:layout_marginEnd">@dimen/item_recomend_margin_size</item>
    </style>
    <style name="AppBaseTheme" parent="@android:style/Theme.Light" />
    <style name="AppTheme" parent="@android:style/Theme.Leui" />
    <style name="HomeTheme" parent="@android:style/Theme.DeviceDefault.NoActionBar.Fullscreen">
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:selectableItemBackground">@android:drawable/item_background_holo_light</item>
        <item name="android:switchPreferenceStyle">@style/LeSwitchPreference</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:colorAccent">@color/theme_color</item>
        <item name="android:preferenceFragmentListStyle">@android:style/PreferenceFragmentList.Leui</item>
    </style>
    <style name="HomeHaveListTheme" parent="@android:style/Theme.DeviceDefault.NoActionBar.Fullscreen">
        <item name="android:listViewStyle">@style/ListViewStyle</item>
        <item name="android:selectableItemBackground">@android:drawable/item_background_holo_light</item>
        <item name="android:switchPreferenceStyle">@style/LeSwitchPreference</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:colorAccent">@color/theme_color</item>
    </style>
    <style name="ListViewStyle" parent="@android:style/Widget.Leui.ListView">
        <item name="android:overScrollMode">always</item>
    </style>
    <style name="LeSwitchPreference" parent="@android:style/Preference.DeviceDefault.SwitchPreference">
        <item name="android:widgetLayout">@android:layout/le_preference_widget_switch</item>
        <item name="android:switchTextOn"></item>
        <item name="android:switchTextOff"></item>
    </style>
    <style name="Thumbnail">
        <item name="android:background">@drawable/image_default</item>
        <item name="android:layout_width">100.0dip</item>
        <item name="android:layout_height">160.0dip</item>
    </style>
    <style name="StyleNameweasdasdsad">
        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:paddingRight">10.0dip</item>
    </style>
    <style name="TitleView">
        <item name="android:background">#8f000000</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">50.0dip</item>
        <item name="android:layout_alignParentTop">true</item>
    </style>
    <style name="TagLayout">
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">640.0px</item>
        <item name="android:src">@drawable/thumbnail_bg</item>
        <item name="android:scaleType">centerCrop</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="CategoryLayout">
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">150.0dip</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="Tag_name_style" parent="@android:style/Widget.Holo.Light.TextView">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="thumbnail_text_style">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@color/white_color</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/bg_shadow</item>
        <item name="android:paddingLeft">8.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">30.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>
    <style name="TitleLayoutStyle">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/title_height</item>
    </style>
    <style name="TitleTextStyle">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">#e3ffffff</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">16.0dip</item>
        <item name="android:layout_width">270.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/title_subscribe</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="BackStyle">
        <item name="android:background">?android:selectableItemBackgroundBorderless</item>
        <item name="android:paddingLeft">16.0dip</item>
        <item name="android:paddingRight">16.0dip</item>
        <item name="android:layout_width">@dimen/actionbar_width</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:src">@drawable/white_back</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:onClick">goBack</item>
    </style>
    <style name="empty_imageView_style">
        <item name="android:layout_width">@dimen/no_info_icon_size</item>
        <item name="android:layout_height">@dimen/no_info_icon_size</item>
        <item name="android:scaleType">centerInside</item>
    </style>
    <style name="empty_textView1_style">
        <item name="android:textSize">@dimen/no_info_textView1_textsize</item>
        <item name="android:textColor">@color/thrid_black_color</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/no_info_textView1_marginTop</item>
        <item name="android:layout_marginBottom">@dimen/no_info_textView1_marginBottom</item>
    </style>
    <style name="empty_textView2_style">
        <item name="android:textSize">@dimen/no_info_textView2_textsize</item>
        <item name="android:textColor">@color/thrid_black_color</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/no_info_textView2_marginTop</item>
        <item name="android:layout_marginBottom">@dimen/no_info_textView2_marginBottom</item>
    </style>
    <style name="empty_button_style">
        <item name="android:textSize">@dimen/no_info_button_textsize</item>
        <item name="android:textColor">@color/selector_button_theme_color</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/selector_button_theme_bg</item>
        <item name="android:layout_width">@dimen/no_info_button_width</item>
        <item name="android:layout_height">@dimen/no_info_button_height</item>
    </style>
    <style name="empty_button2_style">
        <item name="android:textSize">@dimen/no_info_button_textsize</item>
        <item name="android:textColor">@color/selector_button_theme_color</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/selector_button_theme_bg</item>
        <item name="android:layout_width">@dimen/no_info_button_width</item>
        <item name="android:layout_height">@dimen/no_info_button_height</item>
        <item name="android:layout_marginTop">@dimen/no_info_button2_marginTop</item>
    </style>
    <style name="topic_indicator_style">
        <item name="android:background">@drawable/selector_indicator_bg</item>
        <item name="android:layout_width">@dimen/indicator_diameter</item>
        <item name="android:layout_height">@dimen/indicator_diameter</item>
        <item name="android:layout_marginLeft">@dimen/indicator_interval</item>
    </style>
    <style name="RankSelectorStyle">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">#7fffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:background">#00ffffff</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">37.0dip</item>
    </style>
    <style name="HomeTabStyle" parent="@android:style/Widget.TextView">
        <item name="android:textSize">10.0sp</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">3.0dip</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="ListStyle">
        <item name="android:scrollbars">none</item>
    </style>
    <style name="DivideLineStyle">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:background">#d7ffffff</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">1.0px</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="ApplyItemStyle" parent="@android:style/Widget.TextView">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">1.0dip</item>
    </style>
    <style name="ThemeIcon">
        <item name="android:scaleType">fitCenter</item>
    </style>
    <style name="BackTheme">
        <item name="android:windowFullscreen">true</item>
        <item name="android:actionBarStyle">@style/BackActionBar</item>
        <item name="android:windowActionBarOverlay">true</item>
    </style>
    <style name="BackActionBar">
        <item name="android:background">@null</item>
        <item name="android:displayOptions">homeAsUp</item>
        <item name="android:homeAsUpIndicator">@drawable/white_back</item>
    </style>
    <style name="BackTitleTheme">
        <item name="android:windowFullscreen">true</item>
        <item name="android:actionBarStyle">@style/BackTitleActionBar</item>
        <item name="android:actionBarSize">@dimen/actionbar_height</item>
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="SelectableItemBackgroundStyle">
        <item name="android:background">?android:selectableItemBackgroundBorderless</item>
    </style>
    <style name="BackTitleSelectableTheme" parent="@style/BackTitleTheme">
        <item name="android:selectableItemBackground">@android:drawable/item_background_holo_light</item>
    </style>
    <style name="BackTitleActionBar">
        <item name="android:background">@color/black_color</item>
        <item name="android:height">@dimen/actionbar_height</item>
        <item name="android:displayOptions">homeAsUp|showCustom|showTitle</item>
        <item name="android:homeAsUpIndicator">@drawable/white_back</item>
    </style>
    <style name="BackTitleAllCategories" parent="@style/BackTitleTheme">
        <item name="android:title">@string/title_all_categories</item>
    </style>
    <style name="BackTitleAllMineThemes" parent="@style/BackTitleTheme">
        <item name="android:title">@string/title_my_themes</item>
    </style>
    <style name="BackTitleAllMineWallpapers" parent="@style/BackTitleTheme">
        <item name="android:title">@string/title_my_wallpapers</item>
    </style>
    <style name="full_wallpaper_action_button_style">
        <item name="android:layout_width">60.0dip</item>
        <item name="android:layout_height">60.0dip</item>
        <item name="android:scaleType">centerInside</item>
    </style>
</resources>
